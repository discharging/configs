# i3blocks configuration file
#
# The i3blocks man page describes the usage of the binary,
# and its website describes the configuration:
#
#     https://vivien.github.io/i3blocks


# Global properties
separator=true
separator_block_width=10
color=#20f422

[ip]
command=/home/manjaro/.config/i3blocks/scripts/ipadd
interval=30

[mem]
command=/home/manjaro/.config/i3blocks/scripts/mem
#label=Mem: 
interval=30

[cpu]
command=/home/manjaro/.config/i3blocks/scripts/cpu
#label=cpu:
interval=15

[loadavg]
command=/home/manjaro/.config/i3blocks/scripts/load
#label=load:
interval=60

[volume]
command=/home/manjaro/.config/i3blocks/scripts/volume
#label=vol:
interval=1

[bat]
command=/home/manjaro/.config/i3blocks/scripts/bat
#label=BAT:
interval=1

[date_time]
command=/home/manjaro/.config/i3blocks/scripts/date_time
interval=1
